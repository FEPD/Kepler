.coupon-list{
  width: 100%;
  padding: 0 20rpx 120rpx 20rpx;
  box-sizing: border-box;
}
.coupon-box {
  width: 100%;
  height: 200rpx;
  background: #fff;
  margin-top: 20rpx;
  box-sizing: border-box;
  border:1rpx solid #f6f6f6;
  border-left:none;
  display: flex;
  border-radius: 6rpx;
}
.coupon-box .m-box{
  width: 100%;
  height: 110rpx;
  display: block;
  display: flex;
  justify-content:center;
}
.coupon-box .f30{
  font-size: 40rpx;
  height: 110rpx;
  line-height: 186rpx;
  display: block;
  overflow: hidden;
}
.coupon-box .f36{
  font-size: 42rpx;
  height: 110rpx;
  line-height: 160rpx;
  display: block;
  overflow: hidden;

}
.coupon-box .f76{
  font-size: 0;
  height: 110rpx;
  line-height: 152rpx;
  display: block;
  overflow: hidden;
  font-weight:bold;
}
.coupon-box .discount-num {font-size: 88rpx;}
.coupon-box .f76 .dotTxt {
  font-size: 40rpx; 
}
.coupon-box .intTxt{
  font-size: 70rpx;
  line-height: 169rpx;
}
.coupon-box .multi-discount {line-height: 170rpx;}
.multi-discount .intTxt{
  font-size: 60rpx;
}
.coupon-box .f76 .dotTxt {font-size: 40rpx;}
.coupon-box .n-m{
  width: 100%;
  display: block;
  display: flex;
  justify-content:center;
  align-items: center;
  font-size: 24rpx;
  height: 84rpx;
}
.coupon-box .right-box{
  height: 100%;
  width: 420rpx;
  box-sizing: border-box;
  padding-right: 20rpx;
}
.coupon-box .right-l{
  box-sizing: border-box;
  padding-left: 20rpx;


}
.coupon-l{
  position: relative;
  height: 68rpx;
  font-size: 0;
  margin: 17rpx 0 0;
}
.coupon-bg{
  font-size: 20rpx;
  line-height: 30rpx;
  color: #fff;
  height: 30rpx;
  padding: 0 15rpx;
  border-radius: 15rpx;
  display: inline-block;
  margin-right: 10rpx;
  width: auto;
  vertical-align: top;
  position: absolute;
  top: 1px;
}
.coupon-bg::before{
  content: "";
  display: inline-block;
  vertical-align: middle;
  width: 0;
  height: 100%;
  margin-top: 4rpx;
}
.coupon-font{
  height: 69rpx;
  display: inline;
  font-size: 24rpx;
  line-height: 39rpx;
  text-indent:84rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.couponList {
  padding: 0 20rpx 20rpx;
}
.use-t{
  font-size: 22rpx;
  color: #2e2d2d;
}
