.txtOf {text-overflow: hidden; overflow: hidden; white-space: nowrap; text-overflow: ellipsis;}
.arrow-box {width: 24rpx; height: 24rpx; background: #dadada;position: relative;border-radius: 50%;}
.arrow-icon {background: url("http://newbuz.360buyimg.com/jingshop/coupon/arrow-icon.png") no-repeat 0 0;background-size: 100%;width: 8px; height: 4px;position: absolute;top: 50%; left: 50%;margin: -2px 0 0 -4px;background-position: 0 -2px;} 
.moreDisInfo {font-size: 22rpx;border: 1px solid #f6f6f6; color: #585757; line-height: 36rpx;padding: 18rpx 23rpx;background: #fff;box-shadow: 0 15px 35px 0 rgba(0,0,0,0.03);border-radius: 4px; overflow: hidden;}
.topArrow .arrow-icon {background-position: 0 -12px;}
.discount-box {display: flex; justify-content: space-between;align-items: center; width: 100%;}
.discountWrap { box-sizing:border-box;font-size: 20rpx; color: #2e2d2d;margin-top: 10rpx;height: 48rpx;line-height: 48rpx; border-top: 2px dashed #f6f6f6;overflow: hidden;}
.coupon-box .n-m .discountDesc {text-overflow:ellipsis;text-align: center; overflow: hidden; white-space: nowrap;display: block;line-height: 82rpx;}
.coupon-wrap {display: flex;width: 100%; height: 200rpx;border-bottom: 1px solid #f2f2f2;}
.coupon-wrapper {height: auto; flex-direction: column;}
.coupon-box .f30.d-unit {font-size: 30rpx;margin-left: 5rpx;}
.coupon-wrapper .right-box {box-shadow: 0 15px 35px 0 rgba(0,0,0,0.03); }
.r-bot-box{display: flex;justify-content: space-between;align-items: center;margin-top: 12rpx;}
