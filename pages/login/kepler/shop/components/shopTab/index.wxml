<form wx:if="{{key === '1002'}}" style="position:fixed;top:{{100*winScale + statusHeight}}px;z-index: 1;width: 100%" bindsubmit="navChildFormId" class="body-sort">
    <view wx:if="{{key === '1002'}}" class='body body-sort-con'  style='margin-top:0rpx;display:flex;'>
        <button wx:if="{{key === '1002'}}" form-type="submit" class="formBtn  tabitem" catchtap='subClick' data-name='推荐'
                data-key='2001'>
            <text class="subNameNormal" style="color:{{subKey === '2001'?'#F0250F':'#000'}}">推荐</text>
        </button>
        <button wx:if="{{key === '1002'}}" form-type="submit" class="formBtn  tabitem" catchtap='subClick' data-name='销量'
                data-key='2002'>
            <text class="subNameNormal" style="color:{{subKey === '2002'?'#F0250F':'#000'}}">销量</text>
        </button>
        <button wx:if="{{key === '1002'}}" form-type="submit" class="formBtn  tabitem" catchtap='subClick' data-name='新品'
                data-key='2003'>
            <text class="subNameNormal" style="color:{{subKey === '2003'?'#F0250F':'#000'}}">新品</text>
        </button>
        <button wx:if="{{key === '1002'}}" form-type="submit" class="formBtn  tabitem" catchtap='subClick' data-name='价格'
                data-key='2004'>
            <view class='priceFrame'>
                <text class="subNameNormal" style="color:{{subKey === '2004'?'#F0250F':'#000'}}">价格</text>
                <image class='priceIcon' src="{{priceIcon}}"></image>
            </view>
        </button>
    </view>
</form>
