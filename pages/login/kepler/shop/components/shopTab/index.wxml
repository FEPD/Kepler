                </view>
              </form>
            </view>
          </block>
        </scroll-view>
        <view class="banner-search" catchtap="clickSearch" wx:if="{{shopTabs.length}}">
            <image class="banner-search-icon"
                   src="http://njst.360buyimg.com/jdreact/program/shop_search_bar_icon.png"></image>
            <text>搜索</text>
        </view>

    </view>
  </block>
