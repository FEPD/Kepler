.cate-bg{
  position:fixed;
  height:500rpx;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: #fff;
  background-size: cover;
  background-position: center center;
  background-repeat: no-repeat;
}
.cate-bg-banner{
  position:absolute;
  height:500rpx;
  width: 100%;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.6);
  background-size: cover;
  background-position: center center;
  background-repeat: no-repeat;
  z-index: -1;

}
.cate-bg-banner-cover{
  position:absolute;
  height:500rpx;
  width: 100%;
  top: 0;
  left: 0;
  background: linear-gradient(180deg,rgba(0,0,0,0.6) 30%,rgba(248,248,248,1) 100%);
  z-index: -1;
}
.cate-bg-cover{
  height:500rpx;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: linear-gradient(180deg,rgba(0,0,0,0.6) 30%,rgba(248,248,248,1) 100%);
}
